{
  "name": "nasa-header",
  "version": "1.0.0",
  "description": "",
  "contributors": [],
  "scripts": {
    "start": "one-app-runner",
    "prebuild": "npm run clean",
    "build": "bundle-module",
    "watch:build": "npm run build -- --watch",
    "clean": "rimraf build",
    "prepare": "npm run build",
    "test:lint": "eslint --ignore-path .gitignore --ext js,jsx,snap .",
    "test:unit": "jest",
    "test": "npm run test:lint && npm run test:unit"
  },
  "one-amex": {
    "app": {
      "compatibility": "^5.0.0"
    },
    "runner": {
      "dockerImage": "oneamex/one-app-dev:5",
      "moduleMapUrl": "https://cdn.jsdelivr.net/gh/ipreencekmr/CDN@main/nasa/static/module-map.json",
      "modules": [
        "."
      ],
      "rootModuleName": "nasa-root",
      "parrotMiddleware": "./dev.middleware.js",
      "envVars": {
        "ONE_CONFIG_ENV": "development",
        "NODE_ENV": "development",
        "HOLOCRON_MODULE_MAP_URL": "https://cdn.jsdelivr.net/gh/ipreencekmr/CDN@main/nasa/static/module-map.json",
        "ONE_CLIENT_LOCALE_FILENAME": "integration",
        "ONE_CLIENT_CDN_URL": "https://cdn.jsdelivr.net/gh/ipreencekmr/CDN@main/nasa/"
      }
    },
    "bundler": {
      "webpackConfigPath": "webpack.config.js",
      "requiredExternals": [
        "react-intl"
      ]
    }
  },
  "dependencies": {
    "@americanexpress/one-app-ducks": "^4.3.1",
    "@americanexpress/one-app-router": "^1.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.13.6",
    "holocron": "^1.1.2",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-intl": "^5.10.6",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@americanexpress/one-app-bundler": "^6.10.0",
    "@americanexpress/one-app-runner": "^6.10.0",
    "@babel/polyfill": "^7.12.1",
    "amex-jest-preset-react": "^6.1.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-amex": "^3.5.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.15.0",
    "eslint-config-amex": "^15.3.2",
    "fs-extra": "^11.1.1",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "jest-json-schema": "^2.1.0",
    "node-fetch": "^2.6.11",
    "parrot-middleware": "^4.1.1",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "preset": "amex-jest-preset-react",
    "setupFilesAfterEnv": [
      "./test-setup.js"
    ]
  }
}
